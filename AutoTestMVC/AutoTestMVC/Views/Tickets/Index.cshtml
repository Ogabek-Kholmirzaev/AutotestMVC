@model List<Ticket>

@{
    ViewData["Title"] = "Tickets Page";
}

<div class="text-center">
    <h1>Tickets</h1>
    
    @for (var i = 0; i < Model.Count; i++)
    {
        
        var ticketName = $"Ticket{i + 1} ";
        
        if (Model[i].CorrectCount == Model[i].QuestionsCount) 
            ticketName += "✅";
        else 
            ticketName += $"{Model[i].CorrectCount}/{Model[i].QuestionsCount}";

        <a class="btn btn-primary m-1" asp-controller="Examinations" asp-action="Exam" asp-route-ticketId="@Model[i].Id">@ticketName</a> <br/>
    }

</div>